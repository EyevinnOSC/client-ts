{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["readme.md", "packages/core/", "packages/services/", "packages/transcode", "packages/web", "packages/db"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/",
    "template": "node_modules/docdash/",
    "recurse": true,
    "verbose": true
  },
  "docdash": {
    "collapse": true,
    "openGraph": {
      "title": "Eyevinn OSC Javascript SDK Documentation",
      "type": "website",
      "image": "https://www.osaas.io/images/OSC-1200-630.png",
      "url": "https://js.docs.osaas.io"
    },
    "meta": {
      "title": "Eyevinn OSC Javascript SDK Documentation",
      "description": "Javascript client libraries for Open Source Cloud. The Javascript client libraries for Open Source Cloud API are implemented in Typescript, for server-side use with Node and is open source.",
      "keyword": "Open Source Cloud, OSC, Eyevinn, Javascript, SDK, API, client libraries, Typescript, Node.js"
    },
    "menu": {
      "Login": {
        "href": "https://app.osaas.io",
        "target": "_blank",
        "class": "menu-item",
        "id": "login_link"
      },
      "Documentation": {
        "href": "https://docs.osaas.io",
        "target": "_blank",
        "class": "menu-item",
        "id": "docs_link"
      },
      "Examples": {
        "href": "https://github.com/EyevinnOSC/client-ts-examples",
        "target": "_blank",
        "class": "menu-item",
        "id": "examples_link"
      }
    }
  }
}
